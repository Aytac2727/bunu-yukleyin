
@model List<Product>

    <h1>Admin Page</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Name</th>
                <th>Price</th>
                <th>Instock</th>
                <th>Discount</th>
                <th>Photo</th>
                <th>Category</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
        {
            <tr>
                <th>@item.name</th>
                <th>@item.Price</th>
                <th>@item.InStock</th>
                <th>@item.Discount</th>
                <th>
                    <img src="@item.PhotoUrl">
                </th>
            </tr>
            
        }
        </tbody>
    </table>

<h1>Admin </h1>